{
  "singleQuote": true,
  "jsxSingleQuote": true,
  "semi": true,
  "useTabs": false,
  "tabWidth": 2,
  "bracketSpacing": true,
  "trailingComma": "all",
  "bracketSameLine": true,
  "endOfLine": "lf",
  "printWidth": 80,
  "proseWrap": "preserve",
  "importOrder": ["^react(.*)", "antd/(.*)", "<THIRD_PARTY_MODULES>", "@/(.*)", "^[./]"],
  "importOrderSeparation": true,
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "singleQuote": false,
        "quoteProps": "preserve"
      }
    }
  ]
}
